{
  "rules": {
    "reported_items": {
      ".indexOn": ["typeString", "userId", "timestamp"],
      "$itemId": {
        ".indexOn": ["timestamp"]
      }
    },
    "image_embeddings": {
      ".indexOn": ["itemId"]
    },
    "user_items": {
      "$userId": {
        ".indexOn": ["timestamp"]
      }
    }
  }
}
